				},
				{
					"color": "#2b2b2b",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#091057",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#024caa",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "transparent",
					"name": "Transparent",
					"slug": "transparent"
				},
				{
					"color": "#f6fbff",
					"name": "Color 1",
					"slug": "custom-color-1"
				},
				{
					"color": "#4a7cdd",
					"name": "Color 2",
					"slug": "custom-color-2"
				},
				{
					"color": "#000",
					"name": "Color 3",
					"slug": "custom-color-3"
				},
				{
					"color": "#fefff9",
					"name": "Color 4",
					"slug": "custom-color-4"
				},
				{
					"color": "#242424",
					"name": "Color 5",
					"slug": "custom-color-5"
				}
			]
